{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "check-types": {
            "dependsOn": ["build"]
        },
        "@ai16z/agent#check-types": {
            "dependsOn": ["@ai16z/plugin-solana#build"]
        },
        "build": {
            "outputs": ["dist/**"],
            "dependsOn": ["^@ai16z/eliza#build"]
        },
        "@ai16z/plugin-solana#build": {
            "outputs": ["dist/**"],
            "dependsOn": ["@ai16z/plugin-trustdb#build"]
        },
        "@ai16z/plugin-nft-collection-generation#build": {
            "dependsOn": ["@ai16z/plugin-node#build"]
        },
        "eliza-docs#build": {
            "outputs": ["build/**"]
        },
        "dev": {
            "persistent": true,
            "cache": false
        }
    }
}
